{
    "name": "h2flow-js",
    "version": "1.0.0",
    "description": "",
    "main": "main.js",
    "scripts": {
        "start": "yarn dev:no-server",
        "watch": "./node_modules/.bin/webpack -d --watch --colors",
        "build": "webpack -p --config ./webpack/webpack.prod.js && yarn docs",
        "dev": "yarn dev:server",
        "dev:server": "webpack-dev-server ./main.js --hot --watch --colors --open --config ./webpack/webpack.dev.js",
        "dev:no-server": "webpack --progress --colors --watch --open --config ./webpack/webpack.dev.js",
        "debug": "node --inspect --debug-brk ./main.js",
        "sqs": "squarespace-server https://gavin-hackett-jedg.squarespace.com --auth -d ../template",
        "docs": "documentation build --output ./README.md --format md",
        "analyze": "npm run build --env.presets analyze"
    },
    "author": "Ben Obee",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "autoprefixer": "^9.7.6",
        "babel-cli": "^6.26.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-minify-webpack-plugin": "^0.3.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.23.0",
        "compression-webpack-plugin": "^3.1.0",
        "concurrently": "^5.2.0",
        "css-loader": "^3.5.3",
        "documentation": "^13.0.0",
        "es6-promise": "^4.1.1",
        "eslint": "^7.0.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.3.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "eslint-watch": "^6.0.1",
        "exports-loader": "^0.7.0",
        "github-release-cli": "^1.3.1",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^4.3.0",
        "imports-loader": "^0.8.0",
        "json-loader": "^0.5.4",
        "less": "^3.6.0",
        "less-loader": "^6.1.0",
        "mini-css-extract-plugin": "^0.9.0",
        "npm-run-parallel": "^0.6.0",
        "path": "^0.12.7",
        "postcss-loader": "^3.0.0",
        "postcss-smart-import": "^0.7.6",
        "style-loader": "^1.2.1",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^4.15.1",
        "webpack-bundle-analyzer": "^3.7.0",
        "webpack-cli": "^3.0.8",
        "webpack-dev-server": "^3.1.4",
        "webpack-merge": "^4.1.3"
    },
    "dependencies": {
        "component-literal": "^1.5.3"
    }
